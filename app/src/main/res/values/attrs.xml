<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SettingTextView">
        <attr name="stv_switch_on" format="reference|boolean"/>
        <attr name="stv_switch_checked" format="reference|boolean"/>
        <attr name="stv_text" format="reference|string"/>
        <attr name="stv_icon" format="reference|integer"/>
    </declare-styleable>

    <declare-styleable name="GraphicControlTextView">
        <attr name="gct_drawable" format="reference|integer"/>
        <attr name="gct_selected" format="reference|boolean"/>
        <attr name="gct_icon_tint" format="reference|integer"/>
    </declare-styleable>

    <declare-styleable name="SupportTextView">
        <attr name="support_text" format="reference|string"/>
        <attr name="support_icon" format="reference|integer"/>
        <attr name="support_enabled" format="reference|boolean"/>
    </declare-styleable>

    <declare-styleable name="WebLinkTextView">
        <attr name="web_link_text" format="reference|string"/>
        <attr name="web_link_icon" format="reference|integer"/>
    </declare-styleable>

</resources>